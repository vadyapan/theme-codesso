{
  "name": "codesso",
  "displayName": "Codesso",
  "description": "A very clean, minimalistic theme for VS Code",
  "version": "1.3.8",
  "engines": {
    "vscode": "^1.76.0"
  },
  "icon": "icon.png",
  "publisher": "elvados",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "vscode",
    "theme",
    "color-theme",
    "dark",
    "code"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadyapan/theme-codesso.git"
  },
  "contributes": {
    "themes": [
      {
        "label": "Codesso",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-codesso.json"
      },
      {
        "label": "Codesso Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-codesso-contrast.json"
      },
      {
        "label": "Codesso Italics",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-codesso-italics.json"
      },
      {
        "label": "Codesso Contrast Italics",
        "uiTheme": "vs-dark",
        "path": "./themes/theme-codesso-contrast-italics.json"
      },
      {
        "label": "Codesso Light",
        "uiTheme": "vs",
        "path": "./themes/theme-codesso-light.json"
      }
    ]
  }
}
