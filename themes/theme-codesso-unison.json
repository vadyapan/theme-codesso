{
  "name": "Codesso Unison (Beta)",
  "type": "light",
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#e24747"
      }
    },
    {
      "name": "Invalid - Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#e24747"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#e24747"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator",
        "punctuation.separator.pointer-access.c",
        "punctuation.separator.pointer-access.cpp"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#333"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword.operator.expression.in",
        "keyword.operator.expression.instanceof",
        "keyword.operator.expression.of",
        "keyword.operator.expression.typeof",
        "keyword.operator.logical.python",
        "keyword.operator.new",
        "keyword",
        "punctuation.separator.colon.python",
        "storage.modifier",
        "storage.type.function",
        "storage.type"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#333"
      }
    },
    {
      "name": "Delimiters",
      "scope": [
        "punctuation.accessor",
        "punctuation.other",
        "punctuation.section",
        "punctuation.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#333"
      }
    },
    {
      "name": "Misc",
      "scope": ["meta.function.decorator"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Entities",
      "scope": ["entity.name.function", "entity.name.type"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#666"
      }
    },
    {
      "name": "Documents",
      "scope": ["source.json", "source.yaml"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666"
      }
    },
    {
      "name": "Properties",
      "scope": ["support.type.property-name.json", "entity.name.tag.yaml"],
      "settings": {
        "fontStyle": "",
        "foreground": "#000"
      }
    },
    {
      "name": "Strings",
      "scope": ["string.quoted", "string.template"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#666"
      }
    },
    {
      "name": "Strings: Punctuation",
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#666"
      }
    },
    {
      "name": "Extra: Diff Header",
      "scope": "meta.diff.header.git",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#999"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#999"
      }
    },
    {
      "name": "Extra: Diff Deleted",
      "scope": ["markup.deleted.diff", "punctuation.definition.deleted"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Extra: Diff Inserted",
      "scope": ["markup.inserted.diff", "punctuation.definition.inserted"],
      "settings": {
        "fontStyle": "bold"
      }
    }
  ],
