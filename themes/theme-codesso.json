{
  "name": "Codesso",
  "type": "dark",
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#8D8F9E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant"],
      "settings": {
        "foreground": "#709BBD"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.charcter.escape"
      ],
      "settings": {
        "foreground": "#D6B4B4"
      }
    },
    {
      "scope": ["entity.name"],
      "settings": {
        "foreground": "#D6B4B4"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#A0B6E8"
      }
    },
    {
      "scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#AAC9D4"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#D1918F"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#6B6D7C"
      }
    },
    {
      "scope": ["keyword"],
      "settings": {
        "foreground": "#709BBD"
      }
    },
    {
      "scope": ["meta.tag", "meta.brace"],
      "settings": {
        "foreground": "#DEE0EF"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#709BBD"
      }
    },
    {
      "scope": "meta.directive.vue",
      "settings": {
        "foreground": "#AAC9D4"
      }
    },
    {
      "scope": "meta.property-name.css",
      "settings": {
        "foreground": "#A0B6E8"
      }
    },
    {
      "scope": "meta.property-value.css",
      "settings": {
        "foreground": "#A392DC"
      }
    },
    {
      "scope": "meta.tag.other.html",
      "settings": {
        "foreground": "#6B6D7C"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#6B6D7C"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#709BBD"
      }
    },
    {
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#A392DC"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#8D8F9E"
      }
    },
    {
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#709BBD"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#A392DC"
      }
    },
    {
      "scope": ["support"],
      "settings": {
        "foreground": "#A0B6E8"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#A392DC"
      }
    },
    {
      "scope": ["support.function"],
      "settings": {
        "foreground": "#D1918F"
      }
    },
    {
      "scope": ["variable"],
      "settings": {
        "foreground": "#D6B4B4"
      }
    },
    {
      "scope": [
        "variable.other",
        "variable.language",
        "variable.function",
        "variable.argument"
      ],
      "settings": {
        "foreground": "#DEE0EF"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#AAC9D4"
      }
    }
  ],
